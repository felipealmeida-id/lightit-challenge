		"eslint": "^8.16.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.3.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"nodemon": "^3.1.4",
		"prettier": "3.3.2",
		"ts-node": "^10.9.2",
		"typescript": "~5.1.0"
	},
	"dependencies": {
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"multer": "1.4.5-lts.1",
